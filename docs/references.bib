  issn = {0141-8130},
  doi = {10.1016/j.ijbiomac.2024.134245},
  urldate = {2024-10-05},
  abstract = {Polyvinyl alcohol (PVA) is a promising alternative to non-biodegradable flexible packaging materials, and nanocellulose is often used to enhance the properties of PVA films, but the composite films still have poor water resistance and barrier properties. To address this issue, iron ions (Fe3+) were introduced into PVA/cellulose nanofibrils (CNF) films, and Fe3+ formed coordination bonds with carboxyl and hydroxyl groups on the surface of CNF and PVA chains. Therefore, constructing a strong coordination crosslinking network within the film and improving the interfacial interaction between PVA and CNF. The water resistance, mechanical and barrier properties of the crosslinked films were significantly improved. Compared with the un-crosslinked film, the oxygen transmission rate (OTR) was decreased by up to 67~\%, and the water swelling ratio was significantly reduced from 1085~\% to 352~\%. The tensile strength of the film with 1.5~wt\% Fe3+ reached 41.93~MPa, which was 62~\% higher than that of the un-crosslinked film. Furthermore, the composite film demonstrated good recyclability, almost recovering its original mechanical properties in two recycling tests. This simple and effective method for preparing water resistance and barrier films shows potential applications in flexible packaging areas.},
  keywords = {Barrier properties,Ionic crosslinking,Nanocellulose,Polyvinyl alcohol,Water resistance},
  file = {C:\Users\Lukas\Zotero\storage\AEMUI2QN\S0141813024050505.html}
}

